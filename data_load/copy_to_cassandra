-- Create a keyspace
CREATE KEYSPACE IF NOT EXISTS ur WITH REPLICATION = { 'class' : 'SimpleStrategy', 'replication_factor' : '1' };

-- Create a table
CREATE TABLE IF NOT EXISTS ur.register_name_history (
regcode text PRIMARY KEY,
name text,
date_to timestamp
);

TRUNCATE ur.register_name_history ;
-- Insert some data
COPY ur.register_name_history (regcode,date_to,name) FROM './register_name_history.csv' WITH HEADER = TRUE AND DELIMITER=';' and DATETIMEFORMAT='%Y-%m-%d';